{
  "name": "nextjs-ts-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint & npm run lint:md",
    "lint:md": "markdownlint --ignore-path .gitignore . --config ./markdownlint-config.js",
    "lint:md:fix": "markdownlint --fix --ignore-path .gitignore . --config ./markdownlint-config.js",
    "format": "prettier --write \"**/*.{css,js,jsx,ts,tsx,json}\"",
    "prepare": "git config core.hooksPath .hooks"
  },
  "dependencies": {
    "framer-motion": "^11.1.7",
    "next": "^14.2.2",
    "next-sitemap": "^4.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.3",
    "tailwindcss-safe-area": "^0.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@svgr/webpack": "^8.1.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^8.10.0",
    "file-loader": "^6.2.0",
    "lint-staged": "^15.2.2",
    "markdownlint-cli": "^0.39.0",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^12.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svgo-loader": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1"
  }
}
